<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd         http://code.alibabatech.com/schema/dubbo         http://code.alibabatech.com/schema/dubbo/dubbo.xsd         ">
	<!-- 使用multicast广播注册中心暴露发现服务地址 -->
	<dubbo:application name="consumer-web" />
	<!-- 使用multicast广播注册中心暴露发现服务地址 -->
	<dubbo:registry protocol="zookeeper" address="${dubbo.registry.url}" />
	<!-- 引入服务接口 --><!-- 本地调试通过添加 url="dubbo://127.0.0.1:20880/"> -->
	
	<dubbo:reference id="IBannerService"  interface="com.baiyue.services.IBannerService" timeout="${dubbo.consumer.account.timeout}" retries="${dubbo.consumer.retries}" />

</beans>
	